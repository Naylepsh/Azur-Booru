<% include ../partials/header.ejs %>

<img src='<%= post.image.data %>'>
<p>Source: <%= post.source %></p>
<p>Title: <%= post.title %></p>
<p>Tags:</p>
<ul>
  <% for (const tag of post.tags) { %>
    <li><a href='/posts?tags=<%= tag %>'><%= tag %></a></li>
  <% } %>
</ul>

<% include ../partials/footer.ejs %>